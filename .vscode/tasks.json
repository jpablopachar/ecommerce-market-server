{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "dotnet: build",
            "type": "dotnet",
            "task": "build",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$msCompile",
            "options": {
                "cwd": "${workspaceFolder}/ecommerce-market-server"
            },
        },
        {
            "label": "dotnet: restore",
            "type": "dotnet",
            "task": "restore",
            "group": "build",
            "problemMatcher": [],
            "options": {
                "cwd": "${workspaceFolder}/ecommerce-market-server"
            }
        }
    ]
}